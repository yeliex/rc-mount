{
  "name": "rc-mount",
  "version": "2.0.1",
  "description": "react extra component mount decorator",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "src/index.ts",
  "scripts": {
    "test": "tsc --noEmit",
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir es",
    "build:cjs": "tsc --module CommonJS --outDir lib",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf ./dist ./es ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeliex/rc-mount.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-com",
    "antd"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "author": "yeliex <yeliex@yeliex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeliex/rc-mount/issues"
  },
  "homepage": "https://github.com/yeliex/rc-mount#readme",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
